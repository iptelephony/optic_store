 "doctype": "Print Format",
 "font": "Default",
 "html": "{% set gstRego = frappe.db.get_value(\"Company\", doc.company, \"tax_id\") or \"\" %}\n{% set taxRate = frappe.db.get_value(\"Sales Taxes and Charges\", {\"parent\": \"Singapore Tax - YPL\"}, 'rate') %}\n{% macro letterhead() %}\n<div class=\"row\">\n<div class=\"col-xs-7\">\n<img src=\"/files/yak_header_logo.png\" height=\"60px\" style=\"vertical-align: top;\">\n</div>\n<div class=\"col-xs-5\">\n    <div style=\"float:right;\">\n<address>\n  <strong>YAK PTE LTD (UEN:202125309M)</strong><br>\n  55 Siglap Road, B1-23<br>\n Siglap Centre, Singapore 455871<br>\n  <br>\n  \n    {% if gstRego %}\n        GST Reg. No {{ gstRego }}\n    {% endif %}  \n</address>\n    </div>\n</div>\n</div>\n{% endmacro %}\n\n{% macro footer() %}\n<div class=\"container-fluid\" style=\"width: 100% !important; bottom:0; position: absolute; background-color: black; color: white; font-weight: 700; min-height: 1em;\">\n    <div class=\"row d-flex align-items-end justify-content-between\" style=\"font-size: 13px;\">\n        <div class=\"col-xs-12\">\n        </div>\n    </div>\n</div>\n{% endmacro %}\n\n\n<div class=\"container-fluid\" style=\"min-width: 100% !important; min-height: 210mm !important;\">\n{{ letterhead() }}\n\n    <div class=\"row d-flex align-items-end justify-content-end\" style=\"border-bottom: 2px solid black\">\n        <div class=\"col-xs-\">\n            <h2>Official Receipt</h2>\n        </div>\n    </div>\n    \n    <div class=\"row d-flex align-items-end justify-content-between\" style=\"padding-top: 10px; font-size: 13px\">\n        <div class=\"col-xs-6\">\n            <b>Sold to: {{ doc.customer }}</b><br>\n        </div>\n        <div class=\"col-xs-6 text-right\">\n            <span style=\"font-size: 13px\">\n                <b>{{ doc.name }}</b><br>\n                <b>Date: {{ doc.posting_date }} {{ doc.get_formatted(\"posting_time\")  }}</b>\n            </span>\n        </div>\n    </div>\n\n\t<table class=\"table table-bordered text-wrap\" style=\"overflow-x: hidden; table-layout: fixed !important\">\n        <thead>\n            <tr class=\"table-info\" style=\"font-weight: bold;\">\n                <td style=\"width:6%;\">S/No.</td>\n                <td style=\"width:50%;\">Description</td>\n                <td style=\"width:15%;\">Price</td>\n            </tr> \n        </thead>\n        <tbody>    \n\n        {%- for row in doc.items -%}\n        <tr class=\"table-info\">\n            <td><div style=\"float:right;\">{{loop.index}}</div></td>\n            <td>{{ row.item_name }}</td>\n            <td class=\"text-right\">{{ \"$ %.2f\"|format(row.amount|float) }}</td>\n        </tr>\n\n        {%- endfor -%}\n\n        </tbody>\n\n        <tfoot style=\"font-weight: bold;\">\n        {% if doc.additional_discount_percentage > 0 %}\n            <tr class=\"table-info\">\n                <td colspan='2' style=\"text-align: right\">Discount {{ doc.additional_discount_percentage }}%</td>\n                <td class=\"text-right\">{{ \"$ %.2f\"|format(doc.discount_amount|float) }}</td>\n            </tr>\n        {% endif %}\n        {% if gstRego %}\n            {% set gstAmt = taxRate/100.0 * doc.grand_total %}\n            <tr class=\"table-info\">\n                <td colspan='2' style=\"text-align: right\">Net Total</td>\n                <td class=\"text-right\">{{ \"$ %.2f\"|format(doc.grand_total|float) }}</td>\n            </tr>\n            <tr class=\"table-info\">\n                <td colspan='2' style=\"text-align: right\">GST at {{ taxRate }}%</td>\n                <td class=\"text-right\">{{ \"$ %.2f\"|format(gstAmt|float) }}</td>\n            </tr>\n            <tr class=\"table-info\">\n                <td colspan='2' style=\"text-align: right\">Total</td>\n                <td class=\"text-right\">{{ \"$ %.2f\"|format(doc.grand_total + gstAmt |float) }}</td>\n            </tr>\n        {% else %}\n            <tr class=\"table-info\">\n                <td colspan='2' style=\"text-align: right\">Total</td>\n                <td class=\"text-right\">{{ \"$ %.2f\"|format(doc.grand_total |float) }}</td>\n            </tr>\n        {% endif %}\n\n\n\n        </tfoot>\n    </table>\n\n\n</div><!-- .container-fluid -->\n\n{{ footer() }}",
